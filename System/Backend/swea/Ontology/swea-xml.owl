    <!-- http://www.semanticweb.org/abate/ontologies/2022/4/swea#Suitable_heat_on_surface -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/abate/ontologies/2022/4/swea#Suitable_heat_on_surface">
        <rdf:type rdf:resource="http://www.semanticweb.org/abate/ontologies/2022/4/swea#Utilization_Criteria"/>
        <determined_by rdf:resource="http://dbpedia.org/resource/Iran"/>
