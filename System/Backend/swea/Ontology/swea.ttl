#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/abate/ontologies/2022/4/swea#can_be_exploited_in
:can_be_exploited_in rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf owl:topObjectProperty ;
                     owl:inverseOf :can_exploit ;
                     rdfs:domain :Renewable_Energy_Sources ;
                     rdfs:range :Territory ;
                     owl:propertyChainAxiom ( :used_based_on
                                              :determined_by
                                            ) .


###  http://www.semanticweb.org/abate/ontologies/2022/4/swea#can_exploit
:can_exploit rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:domain :Territory ;
             rdfs:range :Renewable_Energy_Sources ;
             owl:propertyChainAxiom ( :determines
                                      :influences_the_use_of
                                    ) .


###  http://www.semanticweb.org/abate/ontologies/2022/4/swea#covered_by
:covered_by rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            owl:inverseOf :located_in ;
            rdfs:domain :Territory ;
            rdfs:range :Company .


###  http://www.semanticweb.org/abate/ontologies/2022/4/swea#determined_by
:determined_by rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               owl:inverseOf :determines ;
               rdfs:domain :Utilization_Criteria ;
               rdfs:range :Territory .


###  http://www.semanticweb.org/abate/ontologies/2022/4/swea#determines
:determines rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:domain :Territory ;
            rdfs:range :Utilization_Criteria .


###  http://www.semanticweb.org/abate/ontologies/2022/4/swea#exploited_in
:exploited_in rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf owl:topObjectProperty ;
              rdfs:domain :Renewable_Energy_Sources ;
              rdfs:range :Territory .


###  http://www.semanticweb.org/abate/ontologies/2022/4/swea#featured_by_device
:featured_by_device rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf owl:topObjectProperty ;
                    rdfs:domain :Utilization_Criteria ;
                    rdfs:range :Device .


###  http://www.semanticweb.org/abate/ontologies/2022/4/swea#featured_by_environment
:featured_by_environment rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf owl:topObjectProperty ;
                         rdfs:domain :Utilization_Criteria ;
                         rdfs:range :Ecosystem .


###  http://www.semanticweb.org/abate/ontologies/2022/4/swea#featured_by_norm
:featured_by_norm rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf owl:topObjectProperty ;
                  rdfs:domain :Utilization_Criteria ;
                  rdfs:range :Legal_Norm .


###  http://www.semanticweb.org/abate/ontologies/2022/4/swea#influences_the_use_of
:influences_the_use_of rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf owl:topObjectProperty ;
                       owl:inverseOf :used_based_on ;
                       rdfs:domain :Utilization_Criteria ;
                       rdfs:range :Renewable_Energy_Sources .


###  http://www.semanticweb.org/abate/ontologies/2022/4/swea#located_in
:located_in rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:domain :Company ;
            rdfs:range :Territory .


###  http://www.semanticweb.org/abate/ontologies/2022/4/swea#measured_by
:measured_by rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:domain :Utilization_Criteria ;
             rdfs:range :Measure .


###  http://www.semanticweb.org/abate/ontologies/2022/4/swea#price
:price rdf:type owl:ObjectProperty ;
       rdfs:subPropertyOf owl:topObjectProperty ;
       rdfs:domain :Utilization_Criteria ;
       rdfs:range :Price .


###  http://www.semanticweb.org/abate/ontologies/2022/4/swea#produce_energy_with
:produce_energy_with rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf owl:topObjectProperty ;
                     rdfs:domain :Company ;
                     rdfs:range :Renewable_Energy_Sources .


###  http://www.semanticweb.org/abate/ontologies/2022/4/swea#used_based_on
:used_based_on rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf owl:topObjectProperty ;
               rdfs:domain :Renewable_Energy_Sources ;
               rdfs:range :Utilization_Criteria .


###  http://www.w3.org/2002/07/owl#topObjectProperty
owl:topObjectProperty rdf:type owl:ObjectProperty ;
                      owl:inverseOf owl:topObjectProperty ;
                      rdf:type owl:SymmetricProperty ,
                               owl:TransitiveProperty ,
                               owl:ReflexiveProperty .


